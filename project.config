[access]
	inheritFrom = releng
[project]
	description = Release Engineering Build infrastructure (Jenkins Job Builder, Vagrant, etc)
[access "refs/*"]
	forgeAuthor = group ldap/gerrit-releng-builder-committers
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group ldap/gerrit-releng-builder-committers
	label-Code-Review = -1..+1 group Non-Interactive releng/builder users
	label-Verified = -1..+1 group Non-Interactive releng/builder users
	label-Verified = -1..+1 group ldap/gerrit-releng-builder-committers
	submit = group ldap/gerrit-releng-builder-committers
	create = group Non-Interactive releng/builder users
	forgeAuthor = group Non-Interactive releng/builder users
	forgeCommitter = group Non-Interactive releng/builder users
	pushMerge = group Non-Interactive releng/builder users
[access "refs/tags/*"]
	create = group ldap/gerrit-releng-builder-committers
	push = group Non-Interactive releng/builder users
	push = group ldap/gerrit-releng-builder-committers
	pushTag = group Non-Interactive releng/builder users
	pushTag = group ldap/gerrit-releng-builder-committers
