[access]
	inheritFrom = releng
[project]
	description = Release Engineering Build infrastructure (Jenkins Job Builder, Packer, etc)
[access "refs/*"]
	read = group GitHub Replication
	owner = group ldap/gerrit-releng-builder-committers
[access "refs/meta/config"]
	push = group Project Owners
[access "refs/heads/master"]
	exclusiveGroupPermissions = submit
	submit = block group Registered Users
